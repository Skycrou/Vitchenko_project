<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="SalesBehavior">
                    <serviceMetadata httpGetEnabled="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="SalesBehavior" name="AnalysisSales.AnalysisSales">
                <endpoint address="AnalysisSales" binding="netTcpBinding" bindingConfiguration=""
                    contract="AnalysisSales.IAnalysisSales" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8080/AnalysisSales" />
                        <add baseAddress="net.tcp://localhost:8085" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>